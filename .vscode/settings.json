{
  "extensions.ignoreRecommendations": true,
  "workbench.startupEditor": "none",
  "editor.fontFamily": "Operator Mono lig",
  //"Ligalex Mono",
  // CaskaydiaCove NERD Font Propo",

  //IBM Plex Mono
  //Cascadia Code,Operator Mono Lig"VictorMono Nerd Font CaskaydiaCove Nerd Font MonoHack Nerd Font Mono
  "editor.fontLigatures": true,
  "editor.fontWeight": 400,
  "editor.cursorWidth": 3,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.formatOnPaste": true,
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 100,
  "editor.formatOnSave": true,
  "editor.wordWrap": "on",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.scrollbar.horizontal": "hidden",
  "[python]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "files.exclude": {
    "**/*.class": true,
    "**/*.exe": true
  },
  "C_Cpp.vcFormat.space.afterCastCloseParenthesis": false,
  "C_Cpp.vcFormat.space.withinCastParentheses": false,
  "settingsSync.ignoredExtensions": ["shan.code-settings-sync"],
  "editor.guides.bracketPairs": "active",
  "git.ignoreMissingGitWarning": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "[cpp]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.autofetch": true,
  "explorer.confirmDelete": false,
  "editor.guides.indentation": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "settingsSync.ignoredSettings": [],
  "vsicons.dontShowNewVersionMessage": true,
  "http.proxyAuthorization": null,
  "http.proxySupport": "on",
  "workbench.sideBar.location": "right",
  "terminal.integrated.env.windows": {},
  "editor.linkedEditing": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "terminal.integrated.defaultProfile.linux": "bash",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "terminal.integrated.fontFamily": "Operator Mono Lig",
  "workbench.productIconTheme": "icons-carbon",
  "editor.renderWhitespace": "boundary",
  "workbench.editor.limit.value": 8,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "explorer.openEditors.visible": 1,
  "git.confirmSync": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "editor.tabSize": 2,
  "editor.minimap.showSlider": "always",
  "editor.minimap.autohide": true,
  "terminal.integrated.tabFocusMode": true,
  "editor.scrollbar.verticalScrollbarSize": 1,
  "editor.occurrencesHighlight": false,
  "editor.selectionHighlight": false,
  "window.titleBarStyle": "custom",

  //left side number appearance
  "editor.folding": false,
  "editor.glyphMargin": true,

  "editor.lineHeight": 1.6, //calculate on the basis of font size * 1.6

  "workbench.layoutControl.enabled": false,

  "window.autoDetectColorScheme": true,
  "workbench.editor.showTabs": false,
  "editor.copyWithSyntaxHighlighting": false,
  "window.newWindowDimensions": "inherit",
  "breadcrumbs.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.emptySelectionClipboard": true,
  "workbench.editor.enablePreview": false,
  "diffEditor.renderSideBySide": false,
  "editor.matchBrackets": "never",
  "editor.lightbulb.enabled": false,
  "editor.overviewRulerBorder": false,
  "problems.decorations.enabled": false,
  "editor.renderControlCharacters": false,
  "workbench.iconTheme": "Monokai Pro (Filter Octagon) Icons",

  "search.exclude": {
    "**/.git": true,
    "**/.github": true,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/tmp": true,
    "**/yarn.lock": true
  },
  "window.menuBarVisibility": "toggle",
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.fontSize": 14,
  "editor.letterSpacing": 0.5,
  "window.zoomLevel": 0.5,
  "workbench.colorTheme": "Catppuccin Mocha",
  "editor.renderLineHighlight": "none"
}
